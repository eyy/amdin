{
  "name": "amdin",
  "version": "0.3.0",
  "main": "server/index",
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm run koa & npm run vue",
    "koa": "DEBUG=amdin cd example/complex && nodemon index",
    "vue": "vue-cli-service serve --open",
    "build": "npm run lint && NODE_ENV=production vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "tape tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyy/amdin.git"
  },
  "peerDependencies": {
    "mongoose": "^5.0.11"
  },
  "dependencies": {
    "koa": "^2.5.0",
    "koa-basic-auth": "^2.0.0",
    "koa-body": "^2.5.0",
    "koa-json-error": "^3.1.2",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@koa/cors": "^2.2.1",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-standard": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.13",
    "cloudinary": "^1.11.0",
    "dotenv": "^5.0.1",
    "koa-logger": "^3.2.0",
    "mongoose": "^5.0.13",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "supertest": "^3.0.0",
    "tape": "^4.9.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vue-slicksort": "^0.1.6",
    "vue-template-compiler": "^2.5.13",
    "vue-toasted": "^1.1.24"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "one-var": 0,
      "new-parens": 0,
      "curly": 0,
      "yoda": 0,
      "brace-style": [
        1,
        "stroustrup"
      ]
    }
  },
  "eslintIgnore": [
    "dist/"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "admin",
    "forms",
    "vue",
    "cms",
    "mongodb",
    "mongodb-admin",
    "mongoose",
    "mongoose-admin"
  ]
}
